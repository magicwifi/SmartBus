 <% provide(:title, 'All users') %>
    <h1>All users</h1>
    <ul class="users">
<% @metatrashes.each do |metatrash| %> 
         <li>
	 <%= gravatar_for metatrash  %>
	<div class="anode">
      <%= link_to metatrash.name+" | "+metatrash.phonenum, metatrash %>
	<div class="bnode"><%= link_to "查看", metatrash, {:class=>"btn btn-primary" } %>  |   <%= link_to "时间轴", detail_metatrash_url(metatrash), {:class=>"btn btn-warning" } %> </div></div>
        </li>
<% end %> 
</ul>
