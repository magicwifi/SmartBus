 <% provide(:title, "Log in") %>
    <h1>系统管理员登录</h1>
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
<%= form_for(:session, url: login_path) do |f| %> 
 <%= f.label :name,"管理员姓名" %>
          <%= f.text_field :name %>
          <%= f.label :password, "密码" %>
          <%= f.password_field :password %>
          <%= f.submit "系统管理员登录", class: "btn btn-primary" %>
<% end %>
      </div>
</div>

