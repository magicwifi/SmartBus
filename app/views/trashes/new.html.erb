<% provide(:title, '注册') %>
<h1>智能垃圾桶注册</h1>

<div class="row">
<div class="col-md-6 col-md-offset-3">
<%= form_for(@trash) do |f| %> 
<%= render 'shared/error_messages' %>
<%= f.label :name,'垃圾桶名称' %>
<%= f.text_field :name, class: 'form-control'  %>
<%= f.label :phonenum,'垃圾桶ID（11位）' %>
<%= f.text_field :phonenum, class: 'form-control'  %>
<%= f.submit "注册完成", class: "btn btn-primary" %>
<% end %>
</div>
</div>
